{
  "name": "Youtube - Save deleted titles",
  "description": "This extension will save the names of all your deleted youtube videos, so you can add them back.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/miniLogo.png",
      "32": "/images/miniLogo.png",
      "48": "/images/miniLogo.png",
      "128": "/images/miniLogo.png"
    }
  },
  "icons": {
    "16": "/images/miniLogo.png",
    "32": "/images/miniLogo.png",
    "48": "/images/miniLogo.png",
    "128": "/images/miniLogo.png"
  }
}
