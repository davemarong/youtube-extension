{
  "name": "Deleted youtube videos",
  "description": "This extension will save the video titles/names of all your deleted youtube videos from 1 playlist, so you can add them back.",
  "version": "0.7.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting","background","contextMenus","webNavigation","tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logoBig.png",
      "32": "/images/logoBig.png",
      "48": "/images/logoBig.png",
      "128": "/images/logoBig.png"
    }
  },
  "icons": {
    "16": "/images/logoBig.png",
    "32": "/images/logoBig.png",
    "48": "/images/logoBig.png",
    "128": "/images/logoBig.png"
  }
}
